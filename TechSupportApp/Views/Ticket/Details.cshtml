@model TechSupportApp.Application.Tickets.Queries.TicketDetails.TicketDetailsVm
<dl class="row">
    <dt class="col-sm-2">
        Id
    </dt>
    <dd class="col-sm-10">
        @Model.Ticket.Id
    </dd>
    <dt class="col-sm-2">
        Issuer
    </dt>
    <dd class="col-sm-10">
        @Model.Ticket.Issuer
    </dd>

    <dt class="col-sm-2">
        Status
    </dt>
    <dd class="col-sm-10">
        @Model.TicketStatusList.Single(c => c.Value == @Model.Ticket.TicketStatus).Name
    </dd>
</dl>
<table class="table table-bordered table-striped table-sm">
 
</table>
   

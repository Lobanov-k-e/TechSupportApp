@model TechSupportApp.Application.Tickets.Commands.CreateTicket.CreateTicket


<form method="post">
    <div class="form-group">
        <label asp-for="Issuer"></label>
        <input class="form-control" asp-for="Issuer" />
    </div>
    <div class="form-group">
        <label asp-for="Issue"></label>
        <input asp-for="Issue" class="form-control" />
    </div>
    <button class="btn btn-primary" type="submit">Send</button>
    <a class="btn btn-danger" asp-action="Index">Cancel</a>
</form>

